<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.nf.generalbook.dao.UserDao">
    <resultMap id="userMap" type="edu.nf.generalbook.entity.User">
        <id property="uId" column="u_id"/>
        <result property="name" column="name"/>
        <result property="account" column="account"/>
        <result property="password" column="password"/>
        <result property="sex" column="sex"/>
        <result property="email" column="email"/>
        <result property="photo" column="photo"/>
        <result property="phone" column="phone"/>
        <result property="state" column="state"/>
    </resultMap>


    <insert id="addUser">
        insert into user(u_id,name,account,password,sex,email,photo,phone,state) values (#{uId},#{name},#{account},#{password},#{sex},#{email},#{photo},#{phone},#{state})
    </insert>

</mapper>